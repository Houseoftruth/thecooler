_pairing":["Game Terrine","Barbecue-griddled Poussin","Clootie Dumpling"],"brewers_tips":"Pre-mix your malts to keep the conversion of starches to sugars even across the mash and give you a more uniform grain bed for lautering and sparging. This is a lot less labour intensive than manually stirring your speciality malts into your mash tun.","contributed_by":"John Jenkman <johnjenkman>"},{"id":264,"name":"Small Batch: East Coast Crush","tagline":"Session Vermont IPA.","first_brewed":"2017","description":"East Coast Crush is a Vermont style pale ale, packaged and served using our custom LIVE beer dispense. A session Vermont IPA, with low background bitterness, loaded with intense juicy mango, pineapple, peach and pine.","image_url":"https://images.punkapi.com/v2/keg.png","abv":4.8,"ibu":25,"target_fg":1010,"target_og":1047,"ebc":15,"srm":8,"ph":4.2,"attenuation_level":79,"volume":{"value":20,"unit":"litres"},"boil_volume":{"value":25,"unit":"litres"},"method":{"mash_temp":[{"temp":{"value":64,"unit":"celsius"},"duration":30}],"fermentation":{"temp":{"value":19,"unit":"celsius"}},"twist":null},"ingredients":{"malt":[{"name":"Pale Ale","amount":{"value":3.24,"unit":"kilograms"}},{"name":"Uncrushed Wheat","amount":{"value":0.6,"unit":"kilograms"}},{"name":"Flaked Oats","amount":{"value":0.24,"unit":"kilograms"}}],"hops":[{"name":"Chinook","amount":{"value":20,"unit":"grams"},"add":"Start","attribute":"Aroma"},{"name":"Mandarina Bavaria","amount":{"value":20,"unit":"grams"},"add":"Start","attribute":"Aroma"},{"name":"Simcoe","amount":{"value":30,"unit":"grams"},"add":"Start","attribute":"Aroma"},{"name":"Citra","amount":{"value":50,"unit":"grams"},"add":"Dry Hop","attribute":"Aroma"},{"name":"Mandarina Bavaria","amount":{"value":50,"unit":"grams"},"add":"Dry Hop","attribute":"Aroma"},{"name":"Mosaic","amount":{"value":50,"unit":"grams"},"add":"Dry Hop","attribute":"Aroma"},{"name":"Simcoe","amount":{"value":50,"unit":"grams"},"add":"Dry Hop","attribute":"Aroma"}],"yeast":